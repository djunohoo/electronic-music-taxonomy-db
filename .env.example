# Electronic Music Taxonomy DB - Example Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
DB_URL=postgresql://user:password@hostname:5432/database_name
DB_HOST=localhost
DB_PORT=5432
DB_NAME=cultural_intelligence
DB_USER=cultural_intel_user
DB_PASSWORD=your_secure_password_here

# Supabase Configuration
SUPABASE_URL=http://localhost:8000
SUPABASE_API_URL=http://localhost:8000
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_KEY=your_service_key_here

# API Configuration
API_HOST=localhost
API_PORT=5000
API_DEBUG=false

# Dashboard Configuration
DASHBOARD_HOST=localhost
DASHBOARD_PORT=8083

# Service Configuration
SERVICE_MODE=production
LOG_LEVEL=INFO
